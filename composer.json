{
    "name": "surfnet/yubikey-api-client-bundle",
    "description": "A Symfony2 bundle to integrate Yubikey OTP validation service.",
    "keywords": [
        "yubikey",
        "yubico",
        "surfnet"
    ],
    "license": "Apache-2.0",
    "autoload": {
        "psr-4": {
            "Surfnet\\YubikeyApiClientBundle\\": "src"
        }
    },
    "require": {
        "php": ">=5.4,<8.0-dev",
        "surfnet/yubikey-api-client": "dev-develop",
        "symfony/config": "^2.7",
        "symfony/dependency-injection": "^2.7",
        "symfony/http-kernel": "^2.7"
    },
    "require-dev": {
        "ibuildings/qa-tools": "~1.1",
        "liip/rmt": "1.1.*",
        "matthiasnoback/symfony-config-test": "0.*",
        "mockery/mockery": "0.9.*",
        "symfony/framework-bundle": ">=2.3,<4",
        "symfony/monolog-bundle": "^2.11"
    }
}
